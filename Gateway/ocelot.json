{
  "Routes": [
    {
      "DownstreamPathTemplate": "/items",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "item-api", "Port": 8080 } ],
      "UpstreamPathTemplate": "/food",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/items/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "item-api", "Port": 8080 } ],
      "UpstreamPathTemplate": "/food/{id}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/order",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [ { "Host": "order-api", "Port": 8080 } ],
      "UpstreamPathTemplate": "/buy",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8000"
  }
}